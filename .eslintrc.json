{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react"],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "indent": ["error", 2, {
        "SwitchCase": 1
      }],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "never"],
      "no-console": 0,
      "no-unreachable": 1,
      "no-unused-vars": ["warn", {"ignoreRestSiblings": true}],
      "react/prop-types": 0
    },
    "globals": {}
  }
  